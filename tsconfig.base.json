{
    "compilerOptions": {
        "incremental": true,
        "target": "es5",
        "module": "es6",
        "jsx": "react",
        "allowJs": true,
        "declaration": true,
        "sourceMap": true,
        "strict": true,
        "moduleResolution": "node",
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "composite": true
    }
}
